<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.philkes.dartsscore">

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.philkes.dartsscore.activites.MainActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.philkes.dartsscore.activites.GameActivity" />
        <activity android:name="com.philkes.dartsscore.activites.PlayerActivity" />
        <activity android:name="com.philkes.dartsscore.activites.GameSettingsActivity" />
        <activity android:name="com.philkes.dartsscore.activites.StatisticsActivity" />
        <activity android:name="com.philkes.dartsscore.activites.OptionsActivity"></activity>
    </application>

</manifest>